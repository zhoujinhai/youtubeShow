<div id="channelList" class="table-responsive">
  <table class="table table-hover">
    <thead>
      <tr class="table-head">
        <th>订阅号名称</th>
        <th>排名</th>
        <th>订阅量</th>
        <th>订阅号ID</th>
        <th>YouTube地址</th>
        <th>更新时间</th>
      </tr>
    </thead>

    <% channelList.forEach(function(channel){ %>
	    <tbody>
	      <tr>
	        <td class="channel-name">
	        	<a href="/channel/<%= channel.channelId %>">
	        		<%= channel.channel %>
	        	</a>
	        </td>
	        <td class="channel-order"><%= channel.order %></td>
	        <td class="channel-subscriber"><%= channel.subscriber %></td>
	        <td><%= channel.channelId %></td>
	        <td>点击：
	        	<a href="<%= channel.channelUrl %>" target="_blank">
	        		<img src="/images/youtube_16.png">
	        	</a>
	        </td>
	        <td><%= channel.currentTime %></td>
	      </tr>
	    </tbody>
	<% }) %>
  </table>
</div>