<div id="channelListByGain" class="table-responsive">
  <table class="table table-hover">
    <thead>
      <tr class="table-head">
        <th>订阅号名称</th>
        <th>排名</th>
        <th>增长量</th>
        <th>当前订阅量</th>
        <th>订阅号ID</th>
        <th>YouTube地址</th>
      </tr>
    </thead>

    <% channelGainList.forEach(function(gainList,index){ %>
    	
	    <tbody>
	      <tr>
	        <td>
	        	<a href="/channel/<%= gainList.channelId %>">
	        		<%= gainList.channelName %>
	        	</a>
	        </td>
	        <td> <%= index+1 %> </td>
	        <td> <%= gainList.gain %> </td>
	        <td> <%= gainList.subscriber %> </td>
	        <td> <%= gainList.channelId %> </td>
	        <td>点击：
	        	<a href="<%= gainList.channelUrl %>" target="_blank">
	        		<img src="/images/youtube_16.png">
	        	</a>
	        </td>
	      </tr>
	    </tbody>
	    
	<% }) %>

  </table>
</div>